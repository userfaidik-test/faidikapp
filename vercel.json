{
  "version": 2,
  "builds": [
    {
      "src": "api/search.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*", 
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/search",
      "dest": "/api/search.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
